        "/api/v1/usage/{key_id}/stats": {
            "get": {
                "tags": [
                    "usage"
                ],
                "summary": "Get Usage Stats",
                "description": "Get usage statistics for an API key.\n\nPeriod format: YYYY-MM (e.g., \"2024-01\")",
                "operationId": "get_usage_stats_api_v1_usage__key_id__stats_get",
                "security": [
                    {
                        "AuthServiceAuth": []
                    }
                ],
                "parameters": [
                    {
                        "name": "key_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid",
                            "title": "Key Id"
                        }
                    },
                    {
                        "name": "period",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string",
                                    "pattern": "^\\d{4}-\\d{2}$"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "title": "Period"
                        }
                    }
                ],
