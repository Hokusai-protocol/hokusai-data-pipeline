    "components": {
        "schemas": {
            "APIKeyCreate": {
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 100,
                        "minLength": 1,
                        "title": "Name"
                    },
                    "service_id": {
                        "type": "string",
                        "pattern": "^(website|prediction|platform)$",
                        "title": "Service Id"
                    },
                    "scopes": {
                        "anyOf": [
                            {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Scopes"
                    },
                    "environment": {
                        "type": "string",
                        "pattern": "^(production|test|development)$",
                        "title": "Environment",
                        "default": "production"
                    },
                    "expires_in_days": {
                        "anyOf": [
                            {
                                "type": "integer",
                                "maximum": 365.0,
                                "minimum": 1.0
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Expires In Days"
                    },
                    "rate_limit_per_hour": {
                        "anyOf": [
                            {
                                "type": "integer",
                                "maximum": 100000.0,
                                "minimum": 10.0
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Rate Limit Per Hour"
                    },
                    "monthly_prediction_limit": {
                        "anyOf": [
                            {
                                "type": "integer",
                                "minimum": 0.0
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Monthly Prediction Limit"
                    },
                    "allowed_ips": {
                        "anyOf": [
                            {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Allowed Ips"
                    },
                    "billing_plan": {
                        "type": "string",
                        "title": "Billing Plan",
                        "default": "free"
                    }
                },
                "type": "object",
                "required": [
                    "name",
                    "service_id"
                ],
                "title": "APIKeyCreate",
                "description": "Schema for creating an API key."
            },
            "APIKeyInfo": {
                "properties": {
                    "key_id": {
                        "type": "string",
                        "format": "uuid",
                        "title": "Key Id"
                    },
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "key_prefix": {
                        "type": "string",
                        "title": "Key Prefix"
                    },
                    "user_id": {
                        "type": "string",
                        "title": "User Id"
                    },
                    "service_id": {
                        "type": "string",
                        "title": "Service Id"
                    },
                    "scopes": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Scopes"
                    },
                    "environment": {
                        "type": "string",
                        "title": "Environment"
                    },
                    "is_active": {
                        "type": "boolean",
                        "title": "Is Active"
                    },
                    "rate_limit_per_hour": {
                        "type": "integer",
                        "title": "Rate Limit Per Hour"
                    },
                    "monthly_prediction_limit": {
                        "anyOf": [
                            {
                                "type": "integer"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Monthly Prediction Limit"
                    },
                    "billing_plan": {
                        "type": "string",
                        "title": "Billing Plan"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "title": "Created At"
                    },
                    "expires_at": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "date-time"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Expires At"
                    },
                    "last_used_at": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "date-time"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Last Used At"
                    }
                },
                "type": "object",
                "required": [
                    "key_id",
                    "name",
                    "key_prefix",
                    "user_id",
                    "service_id",
                    "scopes",
                    "environment",
                    "is_active",
                    "rate_limit_per_hour",
                    "monthly_prediction_limit",
                    "billing_plan",
                    "created_at",
                    "expires_at",
                    "last_used_at"
                ],
                "title": "APIKeyInfo",
                "description": "Schema for API key information (without the actual key)."
            },
            "APIKeyResponse": {
                "properties": {
                    "api_key": {
                        "type": "string",
                        "title": "Api Key",
                        "description": "The full API key (only shown once)"
                    },
                    "key_info": {
                        "$ref": "#/components/schemas/APIKeyInfo"
                    }
                },
                "type": "object",
                "required": [
                    "api_key",
                    "key_info"
                ],
                "title": "APIKeyResponse",
                "description": "Response when creating an API key."
            },
            "APIKeyValidation": {
                "properties": {
                    "api_key": {
                        "type": "string",
                        "title": "Api Key"
                    },
                    "required_scope": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Required Scope"
                    },
                    "service_id": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Service Id"
                    },
                    "client_ip": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Client Ip"
                    }
                },
                "type": "object",
                "required": [
                    "api_key"
                ],
                "title": "APIKeyValidation",
                "description": "Request to validate an API key."
            },
            "BillingInfo": {
                "properties": {
                    "key_id": {
                        "type": "string",
                        "format": "uuid",
                        "title": "Key Id"
                    },
                    "billing_plan": {
                        "type": "string",
                        "title": "Billing Plan"
                    },
                    "current_usage": {
                        "type": "object",
                        "title": "Current Usage"
                    },
                    "monthly_limit": {
                        "anyOf": [
                            {
                                "type": "integer"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Monthly Limit"
                    },
                    "overage_charges": {
                        "type": "number",
                        "title": "Overage Charges",
                        "default": 0.0
                    },
                    "next_billing_date": {
                        "anyOf": [
                            {
                                "type": "string",
                                "format": "date-time"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Next Billing Date"
                    }
                },
                "type": "object",
                "required": [
                    "key_id",
                    "billing_plan",
                    "current_usage",
                    "monthly_limit",
                    "next_billing_date"
                ],
                "title": "BillingInfo",
                "description": "Billing information for an API key."
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "UsageRecord": {
                "properties": {
                    "service_id": {
                        "type": "string",
                        "title": "Service Id"
                    },
                    "endpoint": {
                        "type": "string",
                        "title": "Endpoint"
